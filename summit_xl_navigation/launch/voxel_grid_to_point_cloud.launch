<?xml version="1.0"?>
<launch>
  <node name="voxel_grid_2_point_cloud" pkg="costmap_2d" type="costmap_2d_cloud">
    <remap from="voxel_grid" to="/summit_xl/move_base/local_costmap/obstacle_layer/voxel_grid"/>
    <remap from="voxel_marked_cloud" to="/summit_xl/move_base/local_costmap/obstacle_layer/marked_cloud"/>
    <remap from="voxel_unknown_cloud" to="/summit_xl/move_base/local_costmap/obstacle_layer/unknown_cloud"/>
  </node>
</launch>

